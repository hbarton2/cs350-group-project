CREATE ELEVATOR elevator1 WITH LIMIT 30 SPEED 10 ACCELERATION 0.1
CREATE ELEVATOR elevator2 WITH LIMIT 30 SPEED 10 ACCELERATION 0.1
DECLARE ELEVATOR CONTROLLER elevator_controller1 WITH ELEVATORS elevator1 elevator2
DECLARE BUS bus1 WITH CONTROLLER elevator_controller1
COMMIT
DO elevator_controller1 DEFLECT ELEVATOR 30 DOWN
@WAIT 75
@EXIT