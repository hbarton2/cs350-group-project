CREATE ENGINE engine1 WITH SPEED 10 ACCELERATION 0.2
CREATE ENGINE engine2 WITH SPEED 10 ACCELERATION 0.2
CREATE ENGINE engine3 WITH SPEED 10 ACCELERATION 0.2
DECLARE ENGINE CONTROLLER engine_controller3 WITH ENGINES engine1 engine2 engine3
DECLARE BUS bus1 WITH CONTROLLER engine_controller3
COMMIT
DO engine_controller3 SET POWER 40 engine2
@WAIT 75
@EXIT